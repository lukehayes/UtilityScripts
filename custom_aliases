

# General movement, directories etc.
#
alias asd="cd $HOME"
alias d="cd $HOME/Desktop"
alias dv="cd $HOME/Dev"

# C/C++
#
alias m="make"
alias mc="make clean"
alias a="./bin/app"
alias mm="m && a"

# PHP/Composer
#
alias cpin="composer init"
alias cpis="composer install"
alias cpda="composer dumpautoload"
alias pp="php -S localhost:8000"

# CodeIgniter
#
alias p="php spark"
alias ps="php spark serve"
alias psmr="php spark migrate:refresh"

# Cake
#
alias cake="./bin/cake"
alias cs="cake server"
alias cm="cake migrations"
alias cmm="cake migrations migrate"
alias cms="cake migrations seed"
alias cmr="cake migrations rollback"

