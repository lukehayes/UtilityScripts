# General movement, directories etc.
#
alias asd="cd $HOME"
alias d="cd $HOME/Desktop"
alias dv="cd $HOME/Dev"

# C/C++
#
alias m="make"
alias mc="make clean"
alias a="./bin/app"
alias mm="m && a"

# PHP/Composer
#
alias cpin="composer init"
alias cpis="composer install"
alias cpda="composer dumpautoload"
alias pp="php -S localhost:8000"

# Laravel
#
alias pa="php artisan"
alias pas="php artisan serve"
alias pam="php artisan migrate"

# Symfony
#
alias pbc="php bin/console"
alias sc="symfony console"
alias ss="symfony serve"
alias sd="symfony console doctrine"

# Cake
#
alias cake="./bin/cake"
alias cs="cake server"
alias cm="cake migrations"
alias crm="cake migrations rollback"
